<main class="contenedor contenedor-formularios">
  <h1><%= pagina %></h1>

  <form class="default-form" method="POST" novalidate>
    <div class="campo">
      <label>Nueva contraseña</label>
      <input
        type="password"
        name="password"
        placeholder="Tu nueva contraseña"
      />
    </div>

    <div class="campo enviar">
      <input class="btn btn-rosa" type="submit" value="Guardar cambios" />
    </div>
  </form>
</main>
